{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "noImplicitOverride": true,
    "strict": true,

    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "noEmit": true,
    "preserveConstEnums": true,
    "skipLibCheck": true
  },

  "exclude": [
    "dist",
    "node_modules",
    "server/public",
    "app/dist",
    "app/node_modules",
    "game",
    "download-fonts.ts"
  ]
}
